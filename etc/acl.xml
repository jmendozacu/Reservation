<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Reservation::config_calendar" title="Magenest Reservation Calendar Config" />
                            <resource id="Magenest_Reservation::config_order" title="Magenest Reservation Order Config" />
                        </resource>
                    </resource>
                </resource>

                <resource id="Magenest_Reservation::reservation" title="Magenest Reservation">
                    <resource id="Magenest_Reservation::orders" title="Reservation Orders" sortOrder="10"/>
                    <resource id="Magenest_Reservation::cancel" title="Cancel Requests" sortOrder="20"/>
                    <resource id="Magenest_Reservation::staff_rule" title="Staff Price Rules" sortOrder="30"/>
                    <resource id="Magenest_Reservation::special_date" title="Special Date Price Rules" sortOrder="40"/>
                    <resource id="Magenest_Reservation::reservation_rule" title="Reservation Price Rules" sortOrder="50"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
